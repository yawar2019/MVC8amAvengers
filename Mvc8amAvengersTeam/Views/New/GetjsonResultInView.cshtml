
@{
    ViewBag.Title = "GetjsonResultInView";
}

<h2>GetjsonResultInView</h2>

<table id="table1" class="table table-bordered table-hover">
    <tr>
        <th>EmpId</th>
        <th>EmpName</th>
        <th>EmpSalary</th>
    </tr>
</table>

<script src="~/scripts/jquery-1.10.2.min.js"></script>
<script>
    $.ajax({
        url: "http://localhost:53277/new/GetjsonData2",
        type: "Get",
        datatType: "json",
        success: function (result) {
            $.each(result, function (i, key) {
                $('#table1 tr:last').after("<tr><td>" + key.EmpId + "</td><td>" + key.EmpName + "</td><td>" + key.EmpSalary + "</td></tr>")
            })
        }
    })
</script>


