@model  List<Mvc8amAvengersTeam.Models.EmployeeModel>
@{
    ViewBag.Title = "getSomeData4";
}

<h2>getSomeData4</h2>

<table class="table table-bordered table-responsive">
    <tr>
        <th>EmpId</th>
        <th>EmpName</th>
        <th>EmpSalary</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.EmpId</td>
            <td>@item.EmpName</td>
            <td>@item.EmpSalary</td>
        </tr>
    }
</table>

